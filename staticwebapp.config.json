{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "buildProperties": {
    "appLocation": "/",
    "outputLocation": ".next",
    "buildCommand": "npm run build",
    "installCommand": "npm ci"
  }
} 