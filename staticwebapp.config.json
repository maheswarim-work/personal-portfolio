{
  "routes": [
    {
      "route": "/_next/static/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/_next/static/**/*.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "route": "/_next/static/**/*.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_next/static/*"]
  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "mimeTypes": {
    ".css": "text/css",
    ".js": "application/javascript",
    ".woff2": "font/woff2"
  }
} 